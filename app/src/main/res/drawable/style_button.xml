<?xml version="1.0" encoding="utf-8"?>
<shape

    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <corners
        android:radius="25dp" />
    <solid
        android:color="@color/blue" />

    <padding
        android:left="25dp"
        android:top="15dp"
        android:right="25dp"
        android:bottom="15dp" />

    <stroke
        android:width="2dp"
        android:color="@color/blue"
        android:dashWidth="5dp"
        android:dashGap="0dp" />



</shape>
